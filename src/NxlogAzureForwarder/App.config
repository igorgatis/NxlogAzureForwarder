<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="AzureStorageConnectionString" value="" />
    <add key="AzureTableName" value="NxlogLogsTable" />
    <add key="IncludeExtraColumns" value="True" />
    <add key="AzureQueueName" value="nxlog-logs-queue" />
    <add key="FatQueueMessage" value="True" />
  </appSettings>
  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <remove name="Default" />
        <clear />
        <add name="EventLogListener" type="System.Diagnostics.EventLogTraceListener" initializeData="NxlogAzureForwarder" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>